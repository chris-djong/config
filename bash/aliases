# Aliases
alias vi="nvim"
alias vim="nvim"
alias cd="z"
alias ls="ls --color=auto"
alias ll="ls -al"
alias set_env=". .venv/bin/activate"
alias pbox="boxes -s 74x1 -a c"
# Debian specific 
if command -v batcat >/dev/null 2>&1; then
  alias bat="batcat"
fi
if command -v fdfind >/dev/null 2>&1; then
  alias fd="fdfind"
fi
